<UserControl x:Class="DeCraftLauncher.UIControls.UnimportantLaunchEntryPoint"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DeCraftLauncher.UIControls"
             mc:Ignorable="d" Height="40" MinWidth="516" ToolTip="This class is marked as unimportant, as it is most likely the test class of an external library.">
    <Grid>
        <Label x:Name="classname" Content="class.name" Margin="0,0,68,0" VerticalAlignment="Top" FontSize="22" Foreground="#CCB3B3B3" FontStyle="Italic"/>
        <Button x:Name="launchButton" Content="Launch" Margin="0,5,10,0" Background="{x:Null}" Foreground="White" BorderBrush="#FF292929" Click="launchButton_Click" HorizontalAlignment="Right" Width="53" Height="28" VerticalAlignment="Top"/>

    </Grid>
</UserControl>
